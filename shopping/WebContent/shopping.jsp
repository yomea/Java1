<script>
//瀑布流
function pinterest() {
	var wrap = document.getElementById("waterfall")
	var margin = 10;
	var li = $("#waterfall li");
	var li_W = li[0].offsetWidth + margin;
	var h = [];
	li.css("position", "absolute");
	var n = wrap.offsetWidth / li_W;
	for (var i = 0; i < li.length; i++) {
		li_H = li[i].offsetHeight;
		if (i < n) {
			h[i] = li_H;
			li.eq(i).css("top", 0);
			li.eq(i).css("left", i * li_W);
		}else {
			min_H = Math.min.apply(null, h);
			minKey = getarraykey(h, min_H);
			h[minKey] += li_H + margin;
			li.eq(i).css("top", min_H + margin);
			li.eq(i).css("left", minKey * li_W);
		}
	}
	max = Math.max.apply(null, h);
	$("#waterfall").css("height", max);
}
function getarraykey(s, v) {
	for (k in s) {
		if (s[k] == v) {
			return k;
		}
	}
}
window.onload = function() {
	pinterest();
};
window.onresize = function() {
	pinterest();
};
function getMore() {
	$("#loading").show();
    	var size = '40';
		$("#waterfall").append($(".mypromotion").find("li").slice(size,2*size));
		pinterest();
        $("#loading").hide();
        if($(".mypromotion").find("li").slice(size,2*size).length<1) {
        	 $(".pageMore").hide();
        }
    $("#waterfall img").load(function() {
		$("#loading").hide();
        pinterest();
    });
};
</script>
